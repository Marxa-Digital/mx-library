{
  "name": "marxa-devs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build @marxa/devkit --prod",
    "build:schematics": "cd \"projects/marxa/devkit\" && npm run build && cd ../../../ ",
    "link": "cd \"dist/marxa/devkit\" && npm link",
    "link:lib": "npm link dist/marxa/devkit",
    "publish:package": "cd \"dist/marxa/devkit\" && npm publish",
    "export:styles": "cpx \"projects/marxa/devkit/src/lib/styles/**/*\" \"dist/marxa/devkit/styles\" ",
    "publish": "npm run build:lib && npm run link && npm run link:lib && npm run export:styles && npm run publish:package && npm i -s @marxa/devkit@latest",
    "publish:ng": "npm run build:lib && npm run link &&  npm run export:styles && npm run build:schematics  && npm run link:lib && npm run publish:package && npm i -s @marxa/devkit@latest"
  },
  "private": true,
